		print("en punto")
	elif choice=="to get up":
		print("lavantarse")
	elif choice=="to have a shower":
		print("ducharse")
	elif choice=="to shave":
		print("afeitarse")
	elif choice=="to relax":
		print("relajarse")
	elif choice=="to put on":
		print("ponerse")
		print("to place")
	elif choice=="to become":
		print("ponerse")
	else:
		print("not a word in this code, try again")
